#!/bin/bash

# bin_dir=$svr/bin/linux32/release

bin_dir=$svr/bin

cd $bin_dir

echo "start clean servers"

find -name "core.*" -exec rm -f {} \;
find -name "log_*.*" -exec rm -f {} \;
find -name "nohup.out" -exec rm -f {} \;

exit
