#!/bin/bash

bin_dir=$svr/bin/linux32/release

cd $bin_dir

echo "start run servers"
	echo launching gameserver
		cd gameserver
		nohup ./gameserver &
		cd ../

	echo launching gateserver
		cd gateserver
		# nohup ./gateserver &
		cd ../

	echo launching robot
		cd robot

		for ((i=0;i<20;i++))
		{
			nohup ./robot &
		}

		cd ../
exit
